<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="UDPCommunication.Models"
                   namespace="UDPCommunication.Models.DomainModels">
	<class name="UDPLog">
		<id name="Id" column="Id">
			<generator class="guid" />
		</id>
		<property name="Message" column="Message" />
		<property name="LogDate" column="LogDate" />
		<property name="IpAddress" column="IpAddress" />
		<property name="PortNumber" column="PortNumber" />
		<property name="LogDirection" column="LogDirection" />
	</class>
</hibernate-mapping>